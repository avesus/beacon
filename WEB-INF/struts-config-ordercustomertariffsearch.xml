<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "file:///C:/BUREAUEYE/xml/struts-config_1_3.dtd">
<struts-config>
  <display-name>customer tariff search</display-name>
  <description>This is the Struts configuration file for the customer tariff search</description>
  <form-beans>
    <form-bean name="ListCustomertariffSearchForm" type="com.bureaueye.beacon.form.ListForm"/>
  </form-beans>
  <action-mappings>
    <action path="/EditOrderCustomertariffSearch" type="com.bureaueye.beacon.action.order.EditCustomertariffSearchAction" name="ListCustomertariffSearchForm" scope="session" validate="false">
       <forward name="success" path="/pages/order/CustomertariffSearch.jsp"/>
     </action>
    <action path="/ListOrderCustomertariffSearch" type="com.bureaueye.beacon.action.order.ListCustomertariffSearchAction" name="ListCustomertariffSearchForm" scope="session" validate="false">
      <forward name="success" path="/pages/order/ListCustomertariffSearch.jsp"/>
    </action>
  </action-mappings>
</struts-config>
