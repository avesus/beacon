<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "file:///C:/BUREAUEYE/xml/struts-config_1_3.dtd">
<struts-config>
  <display-name>unit search for onhire</display-name>
  <description>This is the Struts configuration file for the unit search for onhire</description>
  <form-beans>
    <form-bean name="ListUnitSearchForm" type="com.bureaueye.beacon.form.ListForm"/>
  </form-beans>
  <action-mappings>
    <action path="/EditUnitSearchOnhire" type="com.bureaueye.genesys.action.onhire.EditUnitSearchAction" name="ListUnitSearchForm" scope="session" validate="false">
       <forward name="success" path="/pages/onhire/UnitSearch.jsp"/>
     </action>
    <action path="/ListUnitSearchOnhire" type="com.bureaueye.genesys.action.onhire.ListUnitSearchAction" name="ListUnitSearchForm" scope="session" validate="false">
      <forward name="success" path="/pages/onhire/ListUnitSearch.jsp"/>
    </action>
  </action-mappings>
</struts-config>
