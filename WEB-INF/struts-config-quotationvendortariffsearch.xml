<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "file:///C:/BUREAUEYE/xml/struts-config_1_3.dtd">
<struts-config>
  <display-name>vendor tariff search</display-name>
  <description>This is the Struts configuration file for the vendor tariff search</description>
  <form-beans>
    <form-bean name="ListVendortariffSearchForm" type="com.bureaueye.beacon.form.ListForm"/>
  </form-beans>
  <action-mappings>
    <action path="/EditQuotationVendortariffSearch" type="com.bureaueye.beacon.action.quotation.EditVendortariffSearchAction" name="ListVendortariffSearchForm" scope="session" validate="false">
       <forward name="success" path="/pages/quotation/VendortariffSearch.jsp"/>
     </action>
    <action path="/ListQuotationVendortariffSearch" type="com.bureaueye.beacon.action.quotation.ListVendortariffSearchAction" name="ListVendortariffSearchForm" scope="session" validate="false">
      <forward name="success" path="/pages/quotation/ListVendortariffSearch.jsp"/>
    </action>
  </action-mappings>
</struts-config>
