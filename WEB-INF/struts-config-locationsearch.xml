<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "file:///C:/BUREAUEYE/xml/struts-config_1_3.dtd">
<struts-config>
  <display-name>location search</display-name>
  <description>This is the Struts configuration file for the location search</description>
  <form-beans>
    <form-bean name="ListLocationSearchForm" type="com.bureaueye.beacon.form.ListForm"/>
  </form-beans>
  <action-mappings>
    <action path="/EditLocationSearch" type="com.bureaueye.beacon.action.standard.EditLocationSearchAction" name="ListLocationSearchForm" scope="session" validate="false">
       <forward name="success" path="/pages/standard/LocationSearch.jsp"/>
     </action>
    <action path="/ListLocationSearch" type="com.bureaueye.beacon.action.standard.ListLocationSearchAction" name="ListLocationSearchForm" scope="session" validate="false">
      <forward name="success" path="/pages/standard/ListLocationSearch.jsp"/>
    </action>
  </action-mappings>
</struts-config>
